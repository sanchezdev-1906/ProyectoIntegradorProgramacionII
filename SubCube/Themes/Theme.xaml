<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="MainColor">#3A9188</SolidColorBrush>
    <SolidColorBrush x:Key="ColorDark100">#1a1d21</SolidColorBrush>
    <SolidColorBrush x:Key="ColorText">#909090</SolidColorBrush>
    <SolidColorBrush x:Key="ColorChanel">#909090</SolidColorBrush>
    <SolidColorBrush x:Key="ColorTitle">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="ColorLink">#0000ff</SolidColorBrush>

    <Style TargetType="Button" x:Key="Channel">
        <Setter Property="Background" Value="#1a1d21"></Setter>
        <Setter Property="Foreground" Value="#FFFFFF"></Setter>
        <Setter Property="BorderThickness" Value="0"></Setter>
        <Setter Property="FontSize" Value="16px"></Setter>
        <Setter Property="Padding" Value="8px"></Setter>
        <Style.Triggers>
            <Trigger>
                
            </Trigger>
        </Style.Triggers>
    </Style>
</ResourceDictionary>